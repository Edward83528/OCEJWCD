<%@taglib prefix="mst" uri="http://macspeed.net/jsp/ctl/mysimpletag" %>
<html>
    <body>
        <ul>
            <mst:mySwitch var="B">
                <mst:myCase value="A">A</mst:myCase>
                <mst:myCase value="B">B</mst:myCase>
                <mst:myCase value="C">C</mst:myCase>
                <mst:myDefault>Default</mst:myDefault>
            </mst:mySwitch>
        </ul>
    </body>
</html>
